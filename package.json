{
  "name": "clean-react",
  "version": "1.2.0",
  "private": false,
  "description": "Project from course React with TDD and Clean Code (4Dev)",
  "keywords": [
    "TDD",
    "Clean Code",
    "React",
    "ReactJS"
  ],
  "author": {
    "name": "Eduardo Ernesto Corrales Reyna",
    "email": "ecorrales1979@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --open",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test -- --watch",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:ci": "yarn test -- --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "check": "yarn-check -u -s"
  },
  "dependencies": {
    "axios": "0.26.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.5",
    "@testing-library/react": "12.1.3",
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.4.0",
    "@types/node": "17.0.19",
    "@types/react": "17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "clean-webpack-plugin": "^4.0.0",
    "coveralls": "^3.1.1",
    "css-loader": "6.6.0",
    "eslint": "8.9.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-standard": "^5.0.0",
    "git-commit-msg-linter": "^4.0.7",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "27.5.1",
    "jest-localstorage-mock": "2.4.19",
    "lint-staged": "12.3.4",
    "node-sass": "^7.0.1",
    "sass": "1.49.8",
    "sass-loader": "12.6.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "10.5.0",
    "typescript": "^4.5.5",
    "webpack": "5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-middleware": "5.3.1",
    "webpack-dev-server": "4.7.4"
  }
}
